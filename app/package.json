{
  "name": "mixmaster-client",
  "version": "0.0.0",
  "main": "main/main.js",
  "description": "Mixmaster unofficial client application",
  "author": {
    "name": "Kiiow",
    "url": "https://github.com/kiiow"
  },
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "npm-run-all -p electron:serve ng:serve",
    "ng:serve": "ng serve -c dev -o",
    "build": "npm run electron:serve-tsc && ng build --base-href ./",
    "build:prod": "npm run build -- -c production",
    "electron": "electron",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && electron . --serve",
    "electron:local": "npm run build:prod && electron .",
    "electron:build": "npm run build:prod && electron-builder build --publish=never",
    "electron:publish": "npm run build:prod && electron-builder build --publish=always",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "dependencies": {
    "@angular/animations": "^19.2.7",
    "@angular/cdk": "^19.2.3",
    "@angular/common": "^19.2.7",
    "@angular/compiler": "^19.2.7",
    "@angular/core": "^19.2.7",
    "@angular/forms": "^19.2.7",
    "@angular/platform-browser": "^19.2.7",
    "@angular/platform-browser-dynamic": "^19.2.7",
    "@angular/router": "^19.2.7",
    "@fontsource/sora": "^5.2.5",
    "@primeng/themes": "^19.1.0",
    "electron-store": "^10.0.1",
    "primeicons": "^7.0.0",
    "primeng": "^19.1.0",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^19.0.1",
    "@angular-builders/jest": "^19.0.1",
    "@angular/build": "^19.2.8",
    "@angular/cli": "^19.2.8",
    "@angular/compiler-cli": "^19.2.7",
    "@ngx-translate/core": "^16.0.4",
    "@ngx-translate/http-loader": "^16.0.1",
    "@types/jasmine": "~5.1.7",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.1",
    "conventional-changelog-cli": "^5.0.0",
    "electron": "^35.2.0",
    "electron-builder": "^26.0.12",
    "electron-debug": "^4.1.0",
    "jasmine-core": "~5.6.0",
    "jest": "^29.7.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "node-polyfill-webpack-plugin": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3",
    "wait-on": "^8.0.3"
  }
}
